    "def piglatin(word):\n",
    "    a = word[0].lower()\n",
    "    if a in \"aeiou\":\n",
    "        word = word + \"ay\"\n",
    "    else:\n",
    "        word = word[1:] + word[0] + \"ay\"\n",
    "    print(word)"
